{
  "name": "micro-frontend-starter",
  "version": "1.0.0",
  "description": "micro-app",
  "main": "index.js",
  "scripts": {
    "all:install": "npm-run-all --serial install:*",
    "all:start": "npm-run-all --parallel start:*",
    "install:main": "cd main-app && yarn install",
    "install:vue": "cd sub-vue && yarn install",
    "install:vue3": "cd sub-vue3 && yarn install",
    "install:react": "cd sub-react && yarn install",
    "install:html": "cd sub-html && yarn install",
    "start:main": "cd main-app && yarn start",
    "start:vue": "cd sub-vue && yarn start",
    "start:vue3": "cd sub-vue3 && yarn start",
    "start:react": "cd sub-react && yarn start",
    "start:html": "cd sub-html && yarn start",
    "build:main": "cd main-app && yarn build",
    "build:vue": "cd sub-vue && yarn build",
    "build:vue3": "cd sub-vue3 && yarn build",
    "build:react": "cd sub-react && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dingyongcheng2016/micro-frontend-starter.git"
  },
  "keywords": [
    "qiankun.js"
  ],
  "author": "bugcheng@163.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dingyongcheng2016/micro-frontend-starter/issues"
  },
  "homepage": "https://github.com/dingyongcheng2016/micro-frontend-starter#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
